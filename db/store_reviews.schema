create_table "store_reviews", id: :integer, force: :cascade do |t|
  t.integer "user_id", null: false
  t.integer "store_id", null: false

  t.integer "star", null: false
  t.string "description", limit: 512

  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end

add_index "store_reviews", ["user_id"], name: "users_idx", using: :btree
add_index "store_reviews", ["store_id"], name: "stores_idx", using: :btree
