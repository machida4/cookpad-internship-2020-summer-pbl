create_table "user_purchase_histories", id: :integer, force: :cascade do |t|
  t.integer "user_id", null: false
  t.integer "store_id", null: false
  t.integer "menu_id", null: false

  t.integer "status"

  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end

add_index "user_purchase_histories", ["user_id"], name: "users_idx", using: :btree
add_index "user_purchase_histories", ["store_id"], name: "stores_idx", using: :btree
