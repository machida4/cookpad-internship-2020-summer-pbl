create_table "stores", id: :integer, force: :cascade do |t|
  t.string "name", limit: 255, null: false
  t.string "description", limit: 1024, null: false

  t.float "latitude"
  t.float "longitude"

  t.string "image_path"

  # t.integer "status"

  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end
